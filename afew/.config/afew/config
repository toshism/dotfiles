#default filter chain
[SpamFilter]
[KillThreadsFilter]
[ListMailsFilter]
[SentMailsFilter]
sent_tag = sent
[ArchiveSentMailsFilter]

[Filter.1]
message = "Sentry"
query = from:"sentry@sentry02.ams1.uniregistrar.net"
tags = +sentry;+unread;-new

[NotifyFilter]

[Filter.2]
message = "Mark unread, remove new"
tags = +unread;-new

# [InboxFilter]