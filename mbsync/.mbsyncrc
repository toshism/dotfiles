IMAPAccount uniregistry
# Address to connect to
Host imap.uniregistry.com
User tosh@uniregistry.com
# To store the password in an encrypted file use PassCmd instead of Pass
PassCmd "gpg2 -q --batch --for-your-eyes-only --no-tty -d ~/.mailpass.gpg"
#
#AuthMechs LOGIN
SSLType IMAPS
#SSLVersions SSLv3

IMAPStore uniregistry-remote
Account uniregistry

MaildirStore uniregistry-local
SubFolders Legacy
# The trailing "/" is important
Path ~/.mbsync/
Inbox ~/.mbsync/INBOX

Channel uniregistry
Master :uniregistry-remote:
Slave :uniregistry-local:
# Exclude everything under the internal [Gmail] folder, except the interesting folders
#Patterns * ![Gmail]* "[Gmail]/Sent Mail" "[Gmail]/Starred" "[Gmail]/All Mail"
# Or include everything
Patterns *
# Automatically create missing mailboxes, both locally and on the server
Create Both
# Save the synchronization state files in the relevant directory
SyncState *


# pesonal gmail stuff
IMAPAccount toshism
Host imap.gmail.com
User toshism@gmail.com
PassCmd "gpg2 -q --batch --for-your-eyes-only --no-tty -d ~/.toshism.gpg"
SSLType IMAPS
CertificateFile /etc/ssl/certs/ca-certificates.crt

IMAPStore toshism-remote
Account toshism

MaildirStore toshism-local
SubFolders Legacy
# The trailing "/" is important
Path ~/.toshism_mail/
Inbox ~/.toshism_mail/Inbox

Channel toshism
Master :toshism-remote:
Slave :toshism-local:
# Exclude everything under the internal [Gmail] folder, except the interesting folders
Patterns * ![Gmail]* "[Gmail]/Sent Mail" "[Gmail]/Starred" "[Gmail]/All Mail"
# Or include everything
#Patterns *
# Automatically create missing mailboxes, both locally and on the server
Create Both
# Save the synchronization state files in the relevant directory
SyncState *
